{
  "name": "docker-proxy",
  "version": "0.2.1",
  "description": "Docker Proxy.",
  "main": "./lib/docker-proxy.js",
  "directories": {
    "doc": "static/codex",
    "etc": "static/etc",
    "public": "static/public",
    "bin": "bin",
    "lib": "lib"
  },
  "bin": {
    "docker-proxy": "./bin/docker-proxy.js",
    "docker-proxy.entrypoint": "./bin/bash/docker-proxy.entrypoint.sh"
  },
  "scripts": {
    "start": "node --harmony bin/docker-proxy.js start",
    "test": "mocha --ui exports --reporter list test"
  },
  "repository": {
    "url": "git@github.com:UsabilityDynamics/node-docker-proxy.git",
    "type": "git"
  },
  "config": {
    "log": {
      "levels": {
        "info": 0,
        "event": 0,
        "error": 10
      },
      "colors": {
        "info": "red",
        "event": "green",
        "error": "red"
      }
    }
  },
  "preferGlobal": true,
  "dependencies": {
    "async": "^0.9.0",
    "auto": "~0.1.2",
    "axon": "^2.0.0",
    "colors": "^0.6.2",
    "commander": "^2.3.0",
    "debug": "^2.0.0",
    "deep-extend": "^0.2.11",
    "dockerode": "^2.0.3",
    "error-page": "0.0.7",
    "express": "^4.8.7",
    "extend": "^1.3.0",
    "findup-sync": "^0.1.3",
    "http-proxy": "^1.0.3",
    "jade": "^1.6.0",
    "js-yaml": "^3.1.0",
    "jsuck": "0.0.4",
    "lodash": "^2.4.1",
    "lru-cache": "^2.5.0",
    "modelling": "0.0.1",
    "monitor-pid": "^1.4.3",
