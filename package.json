    "node-uuid": "^1.4.1",
    "object-emitter": "^0.1.3",
    "object-settings": "^0.2.0",
    "pidusage": "^0.1.0",
    "redbird": "git://github.com/UsabilityDynamics/node-redbird#develop",
    "request": "^2.40.0",
    "sails-disk": "^0.10.3",
    "sails-memory": "^0.10.2",
    "serve-static": "^1.5.3",
    "string": "^1.9.1",
    "through2": "^0.6.1",
    "utile": "~0.2.1",
    "watch": "^0.11.0",
    "watchr": "^2.4.11",
