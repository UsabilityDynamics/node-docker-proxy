[
  {
    "name"             : "docker-proxy-daemon",
    "script"           : "bin/docker-proxy.js",
    "args"             : "['start']",
    "log_date_format"  : "YYYY-MM-DD HH:mm Z",
    "error_file"       : "/var/log/docker-proxy/docker-proxy.error.log",
    "out_file"         : "/var/log/docker-proxy/daemon.log",
    "pid_file"         : "/var/run/docker-proxy/daemon.pid",
    "instances"        : 3,
    "watch"            : false,
    "merge_logs"       : "true",
    "exec_interpreter" : "node",
    "exec_mode"        : "cluster_mode",
    "env": {
      "NODE_ENV": "production"
    }
  }
]

